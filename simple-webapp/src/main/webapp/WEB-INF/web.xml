<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<!-- Descripteur de deploiement -->
<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  	<!-- Page d'acceuil -->
  	<welcome-file-list>
    	<welcome-file>index.jsp</welcome-file>
  	</welcome-file-list>

  	<servlet>
			<servlet-name>maSimpleServlet</servlet-name>
			<servlet-class>myServlets.Simple</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>maSimpleServlet</servlet-name>
			<url-pattern>/ali</url-pattern>
		</servlet-mapping>
	
		<servlet>
			<servlet-name>E50MServlet</servlet-name>
			<servlet-class>myServlets.Medium</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>E50MServlet</servlet-name>
			<url-pattern>/lyes</url-pattern>
		</servlet-mapping>
		
		<servlet>
			<servlet-name>E50MListServlet</servlet-name>
			<servlet-class>myServlets.ListMedium</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>E50MListServlet</servlet-name>
			<url-pattern>/mehenna</url-pattern>
		</servlet-mapping>
  
</web-app>
